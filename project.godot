;   param=value ; assign values to parameters

config_version=5

[application]

config/name="RunningHalls"
run/main_scene="res://UIs/MenuScene.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[autoload]

Config="*res://Auto/Config.gd"
CurrentGame="*res://Auto/CurrentGame.gd"
